<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
https://raw.githubusercontent.com/Gethe/wow-ui-source/refs/heads/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Frame name="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsColorControlTemplate" inherits="SettingsListElementTemplate" mixin="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsColorControlMixin" virtual="true">
        <Size x="280" y="26"/>
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
        <Frames>
            <Button parentKey="ColorSwatch" inherits="ColorSwatchTemplate" scale="1.6">
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent" relativePoint="CENTER" x="-49" y="0"/>
                </Anchors>
            </Button>
        </Frames>
    </Frame>
    <Frame name="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsInputControlTemplate" inherits="SettingsListElementTemplate" mixin="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsInputControlMixin" virtual="true">
        <Size x="280" y="26"/>
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
        <Frames>
            <EditBox parentKey="InputBox" inherits="InputBoxTemplate" autoFocus="false">
                <Size x="278" y="20"/>
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent" relativePoint="CENTER" x="-74" y="0"/>
                </Anchors>
                <Frames>
                    <Button inherits="UIPanelButtonTemplate">
                        <Size x="20" y="18"/>
                        <Anchors>
                            <Anchor point="RIGHT" relativeKey="$parent.InputBox" relativePoint="RIGHT" x="0" y="0"/>
                        </Anchors>
                        <NormalTexture file="Interface\Buttons\UI-CheckBox-Check"/>
                        <Scripts>
                            <OnLoad>
                                self:GetParent():GetParent().OkayButton = self;
                            </OnLoad>
                        </Scripts>
                    </Button>
                </Frames>
                <Scripts>
                    <OnEnter>
                        self:GetParent().Tooltip:GetScript("OnEnter")(self:GetParent().Tooltip);
                    </OnEnter>
                    <OnLeave>
                        self:GetParent().Tooltip:GetScript("OnLeave")(self:GetParent().Tooltip);
                    </OnLeave>
                </Scripts>
            </EditBox>
        </Frames>
    </Frame>
    <Frame name="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsButtonControlTemplate" inherits="SettingsListElementTemplate" mixin="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsButtonControlMixin" virtual="true">
        <Size x="280" y="26"/>
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
        <Frames>
            <Button parentKey="Button" inherits="UIPanelButtonTemplate">
                <Size x="200" y="26"/>
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent" relativePoint="CENTER" x="-80" y="0"/>
                </Anchors>
            </Button>
        </Frames>
    </Frame>
    <Frame name="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsMultiButtonControlTemplate" inherits="SettingsListElementTemplate" mixin="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsMultiButtonControlMixin" virtual="true">
        <Size x="280" y="26"/>
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
        <Frames>
            <Frame parentKey="ButtonContainer">
                <Size x="370" y="26"/>
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent" relativePoint="CENTER" x="-80" y="0"/>
                </Anchors>
                <Scripts>
                    <OnLoad>
                        self.buttonPool = CreateFramePool('Button', self, 'UIPanelButtonTemplate');
                    </OnLoad>
                </Scripts>
            </Frame>
        </Frames>
    </Frame>
    <Frame name="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsTextTemplate" mixin="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsTextMixin" virtual="true">
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Text" inherits="GameFontNormal" justifyH="LEFT" justifyV="TOP">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="7" y="0"/>
                        <Anchor point="RIGHT" x="-7"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
    <Frame name="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsHeaderTemplate" mixin="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsHeaderMixin" virtual="true">
        <Size y="45"/>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Title" inherits="GameFontHighlightLarge" justifyH="LEFT" justifyV="TOP">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="7" y="-16"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
    </Frame>
    <Frame name="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsSoundSelectorTemplate" inherits="SettingsDropdownControlTemplate" mixin="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsSoundSelectorMixin" virtual="true">
        <Frames>
            <Button parentKey="PreviewIcon">
                <Size x="26" y="26"/>
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent" relativePoint="CENTER" x="-72" y="1"/>
                </Anchors>
                <NormalTexture file="Interface\Common\VoiceChat-Speaker" />
                <Scripts>
                    <OnLoad>
                        self:GetNormalTexture():SetVertexColor(0.8, 0.8, 0.8);
                    </OnLoad>
                    <OnEnter>
                        self:GetNormalTexture():SetVertexColor(1, 1, 1);
                        GameTooltip:SetOwner(self, "ANCHOR_TOP");
                        GameTooltip:SetText(TEXT_TO_SPEECH_PLAY_SAMPLE);
                        GameTooltip:Show();
                    </OnEnter>
                    <OnLeave>
                        self:GetNormalTexture():SetVertexColor(0.8, 0.8, 0.8);
                        GameTooltip:Hide();
                    </OnLeave>
                    <OnClick>
                        self:GetParent():PlaySound();
                    </OnClick>
                </Scripts>
            </Button>
        </Frames>
    </Frame>
    <Frame name="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsExpandTemplate" inherits="SettingsExpandableSectionTemplate" mixin="__NUMY_CONFIG_ADDON_PLACEHOLDER___SettingsExpandMixin" virtual="true">
        <Size y="25" />
    </Frame>
</Ui>